generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int        @id
  files     File[]
  pipelines Pipeline[]
}

model File {
  id          Int    @id
  storageUuid String

  user   User @relation(fields: [userId], references: [id])
  userId Int
}

model Pipeline {
  id       Int    @id
  name     String
  dataJson String

  user   User @relation(fields: [userId], references: [id])
  userId Int
}
